{
    "env": {
        "browser": true,
        "jquery":true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
      "indent":[
        "error",
        2,
        {
          "SwitchCase": 1,
          "VariableDeclarator": {"var": 2, "let": 2, "const": 3}
                        
        }
      ]
    }
}
